{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[],"gpuType":"T4","authorship_tag":"ABX9TyPms9jmC5RbUxgaX0IVhYbT"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"},"accelerator":"GPU"},"cells":[{"cell_type":"code","source":["# ---------------------------\n","# 0️⃣ Mount Google Drive\n","# ---------------------------\n","from google.colab import drive\n","drive.mount('/content/drive')\n","\n","# ---------------------------\n","# 1️⃣ Imports & Config\n","# ---------------------------\n","import os\n","import pandas as pd\n","import numpy as np\n","from sklearn.model_selection import train_test_split\n","import matplotlib.pyplot as plt\n","import tensorflow as tf\n","from tensorflow.keras.models import Sequential\n","from tensorflow.keras.layers import (Conv2D, MaxPooling2D, Flatten, Dense,\n","                                     Dropout, BatchNormalization, GlobalAveragePooling2D)\n","from tensorflow.keras.preprocessing.image import ImageDataGenerator\n","from tensorflow.keras.callbacks import ModelCheckpoint, EarlyStopping, ReduceLROnPlateau\n","\n","# Paths\n","BASE_DIR = \"/content/drive/My Drive/handwriting\"\n","IMG_DIR = os.path.join(BASE_DIR, \"data_subset/data_subset\")  # path to images\n","\n","# Hyperparameters\n","IMG_SIZE = (128, 64)   # (width, height)\n","BATCH_SIZE = 32\n","TOP_K = 50\n","RANDOM_STATE = 42\n","EPOCHS = 30\n","\n","# ---------------------------\n","# 2️⃣ Build DataFrame index\n","# ---------------------------\n","filenames = [f for f in os.listdir(IMG_DIR) if f.lower().endswith(('.png','.jpg','.jpeg'))]\n","\n","def get_writer_from_filename(fname):\n","    # Example IAM format: \"a01-000u-00-00.png\" -> writer = 2nd segment\n","    parts = fname.split('-')\n","    return parts[1] if len(parts) >= 2 else fname.split('_')[0]\n","\n","df = pd.DataFrame({\"image\": filenames})\n","df[\"writer\"] = df[\"image\"].apply(get_writer_from_filename)\n","df[\"path\"] = df[\"image\"].apply(lambda x: os.path.join(IMG_DIR, x))\n","\n","print(\"Total images found:\", len(df))\n","\n","# ---------------------------\n","# 3️⃣ Filter top-K writers\n","# ---------------------------\n","top_writers = df[\"writer\"].value_counts().nlargest(TOP_K).index.tolist()\n","df = df[df[\"writer\"].isin(top_writers)].reset_index(drop=True)\n","print(\"Filtered dataset:\", df.shape)\n","\n","# Map writer -> label\n","writers = sorted(df[\"writer\"].unique())\n","writer2idx = {w:i for i,w in enumerate(writers)}\n","df[\"label\"] = df[\"writer\"].map(writer2idx)\n","\n","# ---------------------------\n","# 4️⃣ Train / Val / Test split\n","# ---------------------------\n","train_df, temp_df = train_test_split(df, test_size=0.2, stratify=df[\"label\"], random_state=RANDOM_STATE)\n","val_df, test_df  = train_test_split(temp_df, test_size=0.5, stratify=temp_df[\"label\"], random_state=RANDOM_STATE)\n","\n","print(\"Train / Val / Test sizes:\", len(train_df), len(val_df), len(test_df))\n","\n","# ---------------------------\n","# 5️⃣ ImageDataGenerators\n","# ---------------------------\n","train_gen = ImageDataGenerator(\n","    rescale=1./255,\n","    rotation_range=5,\n","    width_shift_range=0.05,\n","    height_shift_range=0.05,\n","    shear_range=0.02,\n","    zoom_range=0.05,\n","    fill_mode='nearest'\n",")\n","\n","val_gen = ImageDataGenerator(rescale=1./255)\n","\n","train_flow = train_gen.flow_from_dataframe(\n","    train_df,\n","    x_col=\"path\",\n","    y_col=\"writer\",\n","    target_size=(IMG_SIZE[1], IMG_SIZE[0]),\n","    color_mode=\"rgb\",\n","    class_mode=\"categorical\",\n","    batch_size=BATCH_SIZE,\n","    shuffle=True\n",")\n","\n","val_flow = val_gen.flow_from_dataframe(\n","    val_df,\n","    x_col=\"path\",\n","    y_col=\"writer\",\n","    target_size=(IMG_SIZE[1], IMG_SIZE[0]),\n","    color_mode=\"rgb\",\n","    class_mode=\"categorical\",\n","    batch_size=BATCH_SIZE,\n","    shuffle=False\n",")\n","\n","test_flow = val_gen.flow_from_dataframe(\n","    test_df,\n","    x_col=\"path\",\n","    y_col=\"writer\",\n","    target_size=(IMG_SIZE[1], IMG_SIZE[0]),\n","    color_mode=\"rgb\",\n","    class_mode=\"categorical\",\n","    batch_size=BATCH_SIZE,\n","    shuffle=False\n",")\n","\n","NUM_CLASSES = len(train_flow.class_indices)\n","print(\"Num classes:\", NUM_CLASSES)\n","\n","# ---------------------------\n","# 6️⃣ Build custom CNN\n","# ---------------------------\n","def build_custom_cnn(input_shape=(IMG_SIZE[1], IMG_SIZE[0], 3), n_classes=NUM_CLASSES):\n","    model = Sequential([\n","        Conv2D(32, (3,3), activation='relu', padding='same', input_shape=input_shape),\n","        BatchNormalization(),\n","        Conv2D(32, (3,3), activation='relu', padding='same'),\n","        BatchNormalization(),\n","        MaxPooling2D(2,2),\n","        Dropout(0.25),\n","\n","        Conv2D(64, (3,3), activation='relu', padding='same'),\n","        BatchNormalization(),\n","        Conv2D(64, (3,3), activation='relu', padding='same'),\n","        BatchNormalization(),\n","        MaxPooling2D(2,2),\n","        Dropout(0.3),\n","\n","        Conv2D(128, (3,3), activation='relu', padding='same'),\n","        BatchNormalization(),\n","        Conv2D(128, (3,3), activation='relu', padding='same'),\n","        BatchNormalization(),\n","        MaxPooling2D(2,2),\n","        Dropout(0.4),\n","\n","        GlobalAveragePooling2D(),\n","        Dense(256, activation='relu'),\n","        Dropout(0.5),\n","        Dense(n_classes, activation='softmax')\n","    ])\n","    return model\n","\n","model = build_custom_cnn()\n","model.compile(optimizer=tf.keras.optimizers.Adam(1e-4),\n","              loss='categorical_crossentropy',\n","              metrics=['accuracy'])\n","model.summary()\n","\n","# ---------------------------\n","# 7️⃣ Callbacks & Train\n","# ---------------------------\n","checkpoint = ModelCheckpoint(\"writer_cnn_best.h5\", monitor=\"val_accuracy\", save_best_only=True, verbose=1)\n","earlystop = EarlyStopping(monitor=\"val_loss\", patience=6, restore_best_weights=True, verbose=1)\n","reduce_lr = ReduceLROnPlateau(monitor=\"val_loss\", factor=0.5, patience=3, verbose=1)\n","\n","history = model.fit(\n","    train_flow,\n","    validation_data=val_flow,\n","    epochs=EPOCHS,\n","    callbacks=[checkpoint, earlystop, reduce_lr]\n",")\n","\n","# ---------------------------\n","# 8️⃣ Evaluate on test set\n","# ---------------------------\n","res = model.evaluate(test_flow, verbose=1)\n","print(\"Test loss, acc:\", res)\n","\n","# Confusion matrix\n","import sklearn.metrics as skm\n","y_true = test_flow.classes\n","y_pred_probs = model.predict(test_flow, verbose=1)\n","y_pred = np.argmax(y_pred_probs, axis=1)\n","\n","cm = skm.confusion_matrix(y_true, y_pred)\n","acc = skm.accuracy_score(y_true, y_pred)\n","print(\"Test acc:\", acc)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"ONpjuDfmB94L","outputId":"0daaed74-c2a9-4179-b2fa-4983b44362f9"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Drive already mounted at /content/drive; to attempt to forcibly remount, call drive.mount(\"/content/drive\", force_remount=True).\n","Total images found: 4899\n","Filtered dataset: (1550, 3)\n","Train / Val / Test sizes: 1240 155 155\n","Found 1240 validated image filenames belonging to 50 classes.\n","Found 155 validated image filenames belonging to 50 classes.\n","Found 155 validated image filenames belonging to 50 classes.\n","Num classes: 50\n"]},{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.12/dist-packages/keras/src/layers/convolutional/base_conv.py:113: UserWarning: Do not pass an `input_shape`/`input_dim` argument to a layer. When using Sequential models, prefer using an `Input(shape)` object as the first layer in the model instead.\n","  super().__init__(activity_regularizer=activity_regularizer, **kwargs)\n"]},{"output_type":"display_data","data":{"text/plain":["\